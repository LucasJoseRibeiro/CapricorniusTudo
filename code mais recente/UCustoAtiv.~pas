{$R *.dfm}

procedure TReportCustoAtiv.QRCompositeReport1AddReports(Sender: TObject);
begin
   QRCompositeReport1.Reports.Clear;
   QRCompositeReport1.Reports.Add(QRep1);
   QRCompositeReport1.Reports.Add(QRep2);
end;

procedure TReportCustoAtiv.QRBand2AfterPrint(Sender: TQRCustomBand;
  BandPrinted: Boolean);
begin
     QRep1.NewPage;
end;

end.
